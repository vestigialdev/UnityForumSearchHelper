{
  "manifest_version": 2,
  "name": "UnityForumSearchHelper",
  "version": "0.1",
  "content_scripts": [
   
    {
      "matches": [
        "https://forum.unity.com/forums/*"
      ],
      "js": [
        "forum.unity.com.js"
      ]
    },
    {
      "matches": [
        "https://forum.unity.com/search/*"
      ],
      "js": [
        "search.forum.unity.com.js"
      ]
    }
  ]
}